{
    "name": "checklet",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "cp .env.dev .env.local && next dev",
        "build": "cp .env.prod .env.local && next build",
        "deploy": "docker build -t yourwriterfriend . --platform linux/amd64 && docker tag yourwriterfriend splacorn/yourwriterfriend && docker push splacorn/yourwriterfriend && yarn send-docker-compose",
        "send-docker-compose": "scp -i ~/.ssh/Transistor.pem deploy/docker-compose.yml ec2-user@ec2-3-140-246-97.us-east-2.compute.amazonaws.com:/home/ec2-user/",
        "start": "next start",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "redis": "docker run -d -p 6379:6379 -p 8001:8001 redis/redis-stack:latest"
    },
    "dependencies": {
        "@types/mixpanel": "^2.14.8",
        "antd": "^5.12.2",
        "firebase": "^10.7.1",
        "firebase-admin": "^11.11.1",
        "firebaseui": "^6.1.0",
        "lodash.debounce": "^4.0.8",
        "mixpanel": "^0.18.0",
        "mixpanel-browser": "^2.48.1",
        "next": "14.0.4",
        "openai": "^4.20.1",
        "prettier": "^3.1.1",
        "react": "^18",
        "react-dom": "^18",
        "react-markdown": "^9.0.1",
        "react-toastify": "^9.1.3",
        "redis": "^4.6.11",
        "remark-gfm": "^4.0.0",
        "rich-textarea": "^0.24.4"
    },
    "devDependencies": {
        "@types/lodash.debounce": "^4.0.9",
